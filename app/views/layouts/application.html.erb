  </head>

  <body>
  <div class="main-wrapper row">
    <div class="app col-md-2" id="app">
  <%#= render 'shared/admin_header'  %>

  <%= render 'shared/admin_sidebar' %>
      <article class="content responsive-tables-page">
      </article>
    </div>
    <div class="col-md-10">
      <%= render :partial => 'shared/flash' %>
      <%= yield %>

    </div>
  </div>
  <%# = render 'shared/footer' %>
